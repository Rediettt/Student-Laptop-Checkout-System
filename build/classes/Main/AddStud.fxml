<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.AddStudController">
    <stylesheets>
        <URL value="@addstud.css" />
    </stylesheets>
   <children>
      <AnchorPane prefHeight="404.0" prefWidth="607.0" style="-fx-background-color: #043b5c;">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="55.0" layoutY="26.0" prefHeight="35.0" prefWidth="497.0" text=" Student Laptop Checkout System" textFill="#bd9b19">
               <font>
                  <Font name="Serif Regular" size="24.0" />
               </font>
            </Label>
            <Button fx:id="backbtn" layoutX="414.0" layoutY="332.0" mnemonicParsing="false" onAction="#buttonClicked" style="-fx-background-color: #bd9b19;" text="Back">
               <font>
                  <Font name="Serif Regular" size="14.0" />
               </font>
            </Button>
            <Button fx:id="logoutbtn" layoutX="482.0" layoutY="332.0" mnemonicParsing="false" onAction="#buttonClicked" style="-fx-background-color: #bd9b19;" text="Logout">
               <font>
                  <Font name="Serif Regular" size="14.0" />
               </font>
            </Button>
            <Button fx:id="registerbtn" layoutX="325.0" layoutY="332.0" mnemonicParsing="false" onAction="#changeScreenButtonPushed" style="-fx-background-color: #bd9b19;" text="Register">
               <font>
                  <Font name="Serif Regular" size="14.0" />
               </font>
            </Button>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="220.0" layoutY="73.0" prefHeight="35.0" prefWidth="167.0" text="Register Laptop" textFill="#bd9b19">
               <font>
                  <Font name="Serif Regular" size="20.0" />
               </font>
            </Label>
            <HBox layoutX="126.0" layoutY="115.0" prefHeight="196.0" prefWidth="330.0" spacing="10.0">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                     <children>
                        <Label fx:id="studLbl" text="Student ID:" textFill="#bd9b19">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="brandLbl" text="Brand:" textFill="#bd9b19">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="serialLbl" text="Serial No:" textFill="#bd9b19">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <Label fx:id="gidLbl" text="Guard ID:" textFill="#bd9b19">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox prefHeight="187.0" prefWidth="146.0" spacing="10.0">
                     <children>
                        <TextField fx:id="studTxt" prefHeight="30.0" prefWidth="186.0" promptText="Enter ID">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="brandtxt" prefHeight="30.0" prefWidth="186.0" promptText="Enter Brand of Laptop" />
                        <TextField fx:id="serialTxt" prefHeight="30.0" prefWidth="186.0" promptText="Enter Serial Number" />
                        <TextField fx:id="gidTxt" prefHeight="30.0" prefWidth="167.0" promptText="Enter ID">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </TextField>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
